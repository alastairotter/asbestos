      <script>

        var width = window.outerWidth,
          height = 700;

        if(width > 600) { width = 600; }

        var svg = d3.select("body")
          .append("svg")
          .attr("width", width)
          .attr("height", height)


        d3.csv("asbestos_list.csv", function (data) {
          console.log(data);





            var circles = svg.selectAll("circle");

          function columns(list, xpos, rad) {
            console.log(list);

            circles
            .data(data)
            .enter()
            .append("circle")
            .attr("cx", xpos)
            .attr("cy", function (d, i) {
              return i * 20 + 20;
            })
            .attr("r", function (d) {
              return d[list] * rad;
            })
            .style("fill", "crimson")

            var labels = svg.selectAll("text")
              .data(data)
              .enter()
              .append("text")
              .attr("class", "labels")
              .attr("x", 10)
              .attr("y", function (d, i) {
                return i * 20 + 20;
              })
              .text( function (d) {
                return d.school;
              })






          }

          columns("list1", 300, 7);

          columns("list2", 350, 7);

          columns("list3", 400, 7);

          columns("list4", 450, 7);






        })



      </script>


